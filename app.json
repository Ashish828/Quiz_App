{
  "expo": {
    "name": "QUIZ-APP",
    "slug": "QUIZ-APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Custom-Splash.png",
      "resizeMode": "contain"
    },
    "androidStatusBar": {
      "backgroundColor": "#0066FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
